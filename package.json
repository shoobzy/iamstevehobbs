{
  "name": "iamstevehobbs",
  "version": "1.0.0",
  "description": "Portfolio of Steve Hobbs",
  "main": "",
  "scripts": {
    "start": "parcel src/index.html --no-hmr --out-dir ./dist",
    "prebuild": "shx rm -rf dist/*",
    "build": "parcel build src/index.html --public-url .",
    "postbuild": "react-snap",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "*.scss": "stylelint --syntax=scss"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/shoobzy/iamstevehobbs.git"
  },
  "keywords": [
    "parcel",
    "react"
  ],
  "author": "Steve Hobbs",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.6.1",
    "classnames": "^2.2.6",
    "dotenv": "^8.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-cloudimage-responsive": "^1.6.0",
    "react-dom": "^16.8.6",
    "react-ga": "^2.6.0",
    "react-meta-tags": "^0.7.4",
    "react-pose": "^4.0.8",
    "react-router-dom": "^5.0.1",
    "scss": "^0.2.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "husky": "^3.0.2",
    "lint-staged": "^9.2.1",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "parcel-bundler": "^1.12.3",
    "react-snap": "^1.23.0",
    "sass": "^1.22.9",
    "shx": "^0.3.2",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-order": "^3.0.1",
    "stylelint-scss": "^3.9.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "reactSnap": {
    "source": "./dist",
    "inlineCss": true
  },
  "workbox": {
    "importScripts": "./worker.js",
    "globDirectory": "./dist",
    "globPatterns": "**/*.{css,html,gif,js,jpg,png,svg,webp,webmanifest}"
  }
}
