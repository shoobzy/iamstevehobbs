{
  "name": "iamstevehobbs",
  "version": "1.0.0",
  "description": "Portfolio of Steve Hobbs",
  "main": "",
  "scripts": {
    "start": "parcel src/index.html --no-hmr --out-dir ./dist",
    "prebuild": "shx rm -rf dist/*",
    "build": "parcel build src/index.html --public-url .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "analyze": "source-map-explorer './dist/*.js'"
  },
  "lint-staged": {
    "*.scss": "stylelint --syntax=scss"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/shoobzy/iamstevehobbs.git"
  },
  "keywords": [
    "parcel",
    "react"
  ],
  "author": "Steve Hobbs",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.7.4",
    "classnames": "^2.2.6",
    "cloudinary-react": "^1.6.4",
    "dotenv": "^8.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^2.7.0",
    "react-pose": "^4.0.10",
    "react-router-dom": "^5.1.2",
    "scss": "^0.2.4",
    "source-map-explorer": "^2.4.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.1",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.25.0",
    "shx": "^0.3.2",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.14.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "workbox": {
    "importScripts": "./worker.js",
    "globDirectory": "./dist",
    "globPatterns": "**/*.{css,html,gif,js,jpg,png,svg,webp,webmanifest}"
  }
}
